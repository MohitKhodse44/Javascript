    <script>

        //    ----personal validate-----
        function validatePersonal() {
            let firstName = document.getElementById("firstName").value;
            let lastName = document.getElementById("lastName").value;
            let age = document.getElementById("age").value;
            let city = document.getElementById("city").value;
            let state = document.getElementById("state").value;
            let pinCode = document.getElementById("pinCode").value;
            let hscPercentage = document.getElementById("hscPercentage").value;
            let degreePassOutyear = document.getElementById("degreePassOutYear").value;

            let bankName = document.getElementById("bankName").value;

            var letter = /^[A-Za-z\s]+$/;
            var number = /\d{12}$/;
            var ageNumber = /\d{2}$/;

            if (!firstName.match(letter) && firstName.value == null) {
                alert(`Enter letters in firstName`);
            }
            else if (!lastName.match(letter) && lastName.value == null) {
                alert('Enter letters in lastName');
            }
            else if (!age.match(ageNumber)) {
                alert('Enter number for age')
            }
            else if (!state.match(letter) && state.value == null) {
                alert(`Enter letters for State`);
            }
            else if (!hscPercentage.match(number) && hscPercentage.value == "" && hscPercentage.length > 3) {
                alert('Enter two digit number for hsc% ')
            }
            else if (bankName == "") {
                alert(`Enter BankDetails also`);
            }
            else {
                // submitData();
                alert(`go for account details`)
            }
        }

        //---Account validate------

        function validateAccount() {
            let bankName = document.getElementById("bankName").value;
            let accountNumber = document.getElementById("accountNumber").value;
            let ifscCode = document.getElementById("ifscCode").value;
            let aadharCardNumber = document.getElementById("aadharCardNumber").value;
            let panNumber = document.getElementById("panNumber").value;
            let mobileNumber = document.getElementById("mobileNumber").value;
            let emailId = document.getElementById("emailId").value;

            let fatherName = document.getElementById("fatherName").value;

            var letter = /^[A-Za-z\s]+$/;
            var number = /\d{12}$/;
            if (bankName == "") {
                alert(`Enter BankName please`);
            }
            else if (!accountNumber.match(number) && accountNumber.value == null) {
                alert(`Enter proper ac/no`);
            }
            else if (ifscCode == null) {
                alert(`Enter 12 digit ifsc code`);
            }
            else if (!aadharCardNumber.match(number) && aadharCardNumber.value == null) {
                alert(`Enter proper 12 digit aadhar Number`);
            }
            else if (mobileNumber == "") {
                alert(`Enter 10 digit Number`);
            }
            else if (fatherName == "") {
                alert(`Enter fathername & all Family Details.`);
            }
            else {
                alert(`go for Family details`)
            }
        }

        // ------Emplyoment Validate

        function validateEmployment() {
            let Company = document.getElementById("Company").value;
            let Location = document.getElementById("Location").value;
            let jTitle = document.getElementById("jTitle").value;
            let Experience = document.getElementById("Experience").value;

            let PCompany = document.getElementById("PCompany").value;
            let PLocation = document.getElementById("PLocation").value;
            let PjTitle = document.getElementById("PjTitle").value;
            let PExperience = document.getElementById("PExperience").value;
            let PTechnology = document.getElementById("PTechnology").value;

            let skills = document.getElementById("skills").value;



            var letter = /^[A-Za-z\s]+$/;
            var number = /\d{12}$/;
            if (!Company.match(letter) && Company.value == null) {
                alert(`Enter current company name`);
            }
            else if (!jTitle.match(letter) && jTitle.value == null) {
                alert(`Enter  Current Job Title`);
            }
            else if (!PCompany.match(letter) && PCompany.value == null) {
                alert(`Enter past company name`);
            }
            else if (!PjTitle.match(letter) && PjTitle.value == null) {
                alert(`Enter past job title`);
            }
            else if (skills == "") {
                alert(`fill up the skills box please`);
            }
            else {
                // submitEmployment();
                alert(`go for asset details`)
            }
        }

        // validate Family-------

        function validateFamily() {
            let fatherName = document.getElementById("fatherName").value;
            let Company = document.getElementById("Company").value;

            if (fatherName == "") {
                alert(`Enter Fathername `);
            }
            else if (Company == '') {
                alert(`Enter Current Company Name`);
            }
            else {
                // submitFamily();
                alert(`go for emplyoment details`)
            }
        }

        // ---validate Asset-----

        function validateAsset() {
            let skills = document.getElementById("skills");

            if (skills == "") {
                alert(`Fill up skills box `);
            }
            else {
                submitData();
                submitAccount();
                submitFamily();
                submitEmployment();
                submitAsset();
            }
        }



        //  --------Personal Submit--------
        function submitData() {
            var arrayObjectSecond = localStorage.getItem("Array")
                ? JSON.parse(localStorage.getItem("Array"))
                : [];


            let firstName = document.getElementById("firstName").value;
            let lastName = document.getElementById("lastName").value;
            let age = document.getElementById("age").value;
            let city = document.getElementById("city").value;
            let state = document.getElementById("state").value;
            let pinCode = document.getElementById("pinCode").value;
            let hscPercentage = document.getElementById("hscPercentage").value;
            let degreePassOutyear = document.getElementById("degreePassOutYear").value;

            var dataObject = {
                firstName: firstName,
                lastName: lastName,
                age: age,
                city: city,
                state: state,
                pinCode: pinCode,
                hscPercentage: hscPercentage,
                degreePassOutyear: degreePassOutyear,
            };


            arrayObjectSecond.push(dataObject);
            localStorage.setItem("Array", JSON.stringify(arrayObjectSecond));
            // document.getElementById("myform").reset();

            document.getElementById("firstName").value = "";
            document.getElementById("lastName").value = "";
            document.getElementById("age").value = "";
            document.getElementById("city").value = "";
            document.getElementById("state").value = "";
            document.getElementById("pinCode").value = "";
            document.getElementById("hscPercentage").value = "";
            document.getElementById("degreePassOutYear").value = "";
        }
        //-----------------Account submit------------------
        function submitAccount() {
            var arrayObjectAccount = localStorage.getItem("accountData")
                ? JSON.parse(localStorage.getItem("accountData"))
                : [];
            let dataObjectAccount = {
                bankName: document.getElementById("bankName").value,
                accountNumber: document.getElementById("accountNumber").value,
                ifscCode: document.getElementById("ifscCode").value,
                aadharCardNumber: document.getElementById("aadharCardNumber").value,
                panNumber: document.getElementById("panNumber").value,
                mobileNumber: document.getElementById("mobileNumber").value,
                emailId: document.getElementById("emailId").value,
            };
            arrayObjectAccount.push(dataObjectAccount);
            localStorage.setItem("accountData", JSON.stringify(arrayObjectAccount));
            document.getElementById("bankName").value = "";
            document.getElementById("accountNumber").value = "";
            document.getElementById("ifscCode").value = "";
            document.getElementById("aadharCardNumber").value = "";
            document.getElementById("panNumber").value = "";
            document.getElementById("mobileNumber").value = "";
            document.getElementById("emailId").value = "";
        }


        // ------Family----- submit--------
        function submitFamily() {
            var arrayObjectFamily = localStorage.getItem("familyData") ? JSON.parse(localStorage.getItem("familyData")) : [];
            let dataObjectFamily = {
                fatherName: document.getElementById("fatherName").value,
                motherName: document.getElementById("motherName").value,
                maritalStatus: document.getElementById("maritalStatus").value,
                familyMembers: document.getElementById("familyMembers").value,
                permanentAddress: document.getElementById("permanentAddress").value,
            };
            arrayObjectFamily.push(dataObjectFamily);
            localStorage.setItem("familyData", JSON.stringify(arrayObjectFamily));
            document.getElementById("fatherName").value = "";
            document.getElementById("motherName").value = "";
            document.getElementById("maritalStatus").value = "";
            document.getElementById("familyMembers").value = "";
            document.getElementById("permanentAddress").value = "";
        }


        //  ---------Emplyoment submit-------


        function submitEmployment() {
            let employmentArray = localStorage.getItem('EmploymentData') ? JSON.parse(localStorage.getItem('EmploymentData')) : [];

            var employment = {
                Company: document.getElementById("Company").value,
                Location: document.getElementById("Location").value,
                jTitle: document.getElementById("jTitle").value,
                Experience: document.getElementById("Experience").value,

                PCompany: document.getElementById("PCompany").value,
                PLocation: document.getElementById("PLocation").value,
                PjTitle: document.getElementById("PjTitle").value,
                PExperience: document.getElementById("PExperience").value,
                PTechnology: document.getElementById("PTechnology").value
            }
            employmentArray.push(employment);
            localStorage.setItem('EmploymentData', JSON.stringify(employmentArray));
            document.getElementById("Company").value = "";
            document.getElementById("Location").value = "";
            document.getElementById("jTitle").value = "";
            document.getElementById("Experience").value = "";

            document.getElementById("PCompany").value = "";
            document.getElementById("PLocation").value = ""
            document.getElementById("PjTitle").value = "";
            document.getElementById("PExperience").value = "";
            document.getElementById("PTechnology").value = "";

            alert(`Data Submitted Successfully`)
        }


        //   --------asset------

        function submitAsset() {
            var arrayAsset = localStorage.getItem("asset") ? JSON.parse(localStorage.getItem("asset")) : [];
            let dataObjectAsset = {
                bio: document.getElementById("bio").value,
                skills: document.getElementById("skills").value,

            }

            arrayAsset.push(dataObjectAsset);
            localStorage.setItem("asset", JSON.stringify(arrayAsset));

        }




